{
  "name": "cherry-pick",
  "displayName": "Cherry pick",
  "version": "1.2.0",
  "description": "Handpick scraping assistance",
  "author": "zan-keith",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.15.0",
    "@tailwindcss/postcss": "^4.1.13",
    "hook": "link:@plasmohq/storage/hook",
    "lucide-react": "^0.543.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.17",
    "typescript": "5.3.3"
  },
  "manifest": {
    "manifest_version": 3,
    "version": "1.2.0",
    "key": "$CRX_KEY",
    "host_permissions": [
      "https://*/*"
    ],
    "browser_specific_settings": {
      "gecko": {
        "id": "cherry-pick@zan-keith.com",
        "data_collection_permissions": {
          "required": [
            "websiteContent"
          ],
          "optional": []
        }
      }
    }

  }
}
